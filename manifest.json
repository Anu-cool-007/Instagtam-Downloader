{
	"name": "Instagram Downloader",
	"version": "0.1",
	"description": "TODO",
	"manifest_version": 2,
	"icons": {
		"16": "images/replacely16.png",
		"32": "images/replacely32.png",
		"48": "images/replacely48.png",
		"128": "images/replacely128.png"
	},
	"background": {
		"scripts": [
			"./background.js"
		],
		"persistent": false
	},
	"options_page": "./options.html",
	"browser_action": {
		"default_popup": "./popup.html"
	},
	"permissions": [
		"tabs",
		"*://*.instagram.com/",
		"downloads"
	]
}